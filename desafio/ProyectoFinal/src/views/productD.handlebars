<div class="container" style="margin-top: 50px;">
    <div class="row">
        <div class="gallery-wrapper">
            <div class="thumb">
                <ul>
                    {{#each data.thumbnail}}
                        <li><img src="{{this}}" alt="" onclick="gallery(this)"></li>
                    {{/each}}
                </ul>
            </div>
            <div id="img-container" class="prim-image">
            <!--    <img src="/img/MOSADEnz97iiFFXEB8oLUEbI3SHLFt.jpg" alt="">-->
                <img id="prim-image" src="{{data.thumbnail.[0]}}" alt="">
            </div>
        </div>

        <div class="gallery-wrapper g">
            <div class="row justify-content-md-center">
                <div class="col-12">
                    <h2>{{data.title}}</h2>
                    <p>{{data.description}}</p>
                    <p>{{data.price}}</p>
                </div>
            </div>
        </div>
    </div>
</div>
<link rel="stylesheet" href="/css/estilo.css">
<script src="https://cdn.jsdelivr.net/npm/js-image-zoom/js-image-zoom.min.js"></script>
<script src="/js/productD.js"></script>

